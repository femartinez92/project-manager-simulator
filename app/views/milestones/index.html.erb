<%- if @milestones.length > 0 %>
  <%= render 'milestones_list', locals: { milestones: @milestones } %>
<% end -%>
<br>
 <%= link_to 'Ver precedencias de tareas', precedents_path, class: 'button tiny info' %>
 <%= link_to 'Volver a proyecto', project_path(@project), class: 'button tiny secondary' %>
<br>
<%- if @project.modificable? and not(@admin)%>
  <%= render 'add_milestones_list', locals: { milestones: @posible_milestones } %>
<% end -%>
<br>
<%- if @admin %>
  <%= link_to 'Agregar hito nuevo', new_project_milestone_path, class: 'button tiny info' %>
<% end -%>